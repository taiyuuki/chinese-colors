{
    "root": true,
    "extends": [
        "@taiyuuki/eslint-config-ts"
    ],
    "rules": {
        "import/no-unresolved": [
            "warn",
            {
                "ignore": [
                    "vscode"
                ]
            }
        ]
    },
    "ignorePatterns": [
        "out",
        "dist",
        "**/*.d.ts"
    ]
}